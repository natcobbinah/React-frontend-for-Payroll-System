erstoDisable);\r\n    }\r\n\r\n    onDelete(id){\r\n        axios.get(`${PATH_DELETEUSER}?${PARAM_DELETE}${id}`)\r\n        this.componentDidMount();\r\n    }\r\n\r\n    //'/test/sendemail/{to}/{subject}/{message}' MAILTRAP CILENT WORKS\r\n    //http://localhost:2345/v1/test/sendLoginCredentials/{to}/{content} GMAIL CLIENT\r\n    onSendLoginDetails(email,password){\r\n        const{loginsetnFailure} = this.state;\r\n        /* axios.get(`${PATH_SENDLOGINDETAILS}/${email}/${PATH_LOGINMESSAGE}/${password}`)\r\n        .then(loginsentSuccess => this.setState({loginsentSuccess: loginsentSuccess.data}))\r\n        .catch(loginsetnFailure => this.setState({loginsetnFailure})) */\r\n        \r\n        axios.get(`${PATH_SENDLOGINDETAILS_GMAIL}/${email}/${password}`)\r\n        .then(loginsentSuccess => this.setState({loginsentSuccess: loginsentSuccess.data}))\r\n        .catch(loginsetnFailure => this.setState({loginsetnFailure}))\r\n        console.log(loginsetnFailure);\r\n    }\r\n\r\n    componentDidMount(){\r\n        this.fetchAllUsers();\r\n    }\r\n    \r\n    render(){\r\n        const{searchUser,error,page = 0,result,checked,loginsentSuccess,loginsetnFailure} = this.state;\r\n        return(\r\n            <div>\r\n                <Search value={searchUser} onChange={this.onSearchChange}>Search</Search>\r\n                <Button onClick={() => this.fetchAllUsers(page - 1)} type=\"button\" className=\"btn btn-success\">\r\n                    PreviousRecord\r\n                </Button>  \r\n                <Button onClick={() => this.fetchAllUsers(page + 1)} type=\"button\" className=\"btn btn-primary\">\r\n                    NextRecord\r\n                </Button>\r\n                {  error? \r\n                    <div className=\"alert alert-danger\" role=\"alert\">\r\n                    <p>Error fetching  records Server might be down</p>\r\n                  </div> : null\r\n                }\r\n\r\n                { loginsetnFailure? \r\n                    <div className=\"alert alert-danger\" role=\"alert\">\r\n                    <p>Error sending login details</p>\r\n                  </div> : null\r\n                }\r\n\r\n                { loginsentSuccess? \r\n                    <div className=\"alert alert-success\" role=\"alert\">\r\n                    <p>Login details sent successfully</p>\r\n                  </div> : null\r\n                }\r\n\r\n                { result ?\r\n                        <Table list={result.content} pattern={searchUser} onDelete={this.onDelete} onDisableUsers={this.onDisableUsers} \r\n                        onCheckboxSelected={this.onCheckboxSelected} checboxsetState={checked} onSendLoginDetails={this.onSendLoginDetails}\r\n                       />\r\n                : null\r\n                }   \r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nclass Search extends Component{\r\n    render(){\r\n        const {value,onChange,onSubmit,children} = this.props;\r\n        return(\r\n            <form className=\"form-inline mr-5 my-2 my-lg-0 mt-5 py-4\" onSubmit={onSubmit}>\r\n                 {children}<input type=\"text\" value={value} onChange={onChange} className=\"form-control form-control-lg\" placeholder=\"Search for user by name\" aria-label=\"Search\"/>\r\n            </form>\r\n        );\r\n    }\r\n}\r\n\r\nclass Table extends Component{\r\n    render(){\r\n        const{pattern,list,onDelete,onDisableUsers,onCheckboxSelected,checboxsetState,onSendLoginDetails,\r\n            } = this.props;\r\n        return(\r\n            <div className=\"table-responsive table-hover table-striped\">\r\n                <table className=\"table\">\r\n    ...             <thead className=\"thead-dark\">\r\n                        <tr>\r\n                         <th scope=\"col\">Email</th>\r\n                         <th scope=\"col\">EmployeeID</th>\r\n                         <th scope=\"col\">EmployeeLvl</th>\r\n                         <th scope=\"col\">PhoneNo</th>\r\n                         <th scope=\"col\">BankAccno</th>\r\n                         <th scope=\"col\">Gender</th>\r\n                         <th scope=\"col\">HireDate</th>\r\n                         <th scope=\"col\"></th>\r\n                         <th scope=\"col\">ACTION</th>\r\n                         <th scope=\"col\">ACTION</th>\r\n                         <th scope=\"col\">ACTION</th>\r\n                         <th scope=\"col\">ACTION</th>\r\n                         <th scope=\"col\">ACTION</th>\r\n                        </tr>\r\n                     </thead>\r\n                     <tbody>\r\n                         {list.filter(searchedForRecord(pattern)).map(user => \r\n                        <tr key={user.id}>\r\n                          <td>{user.email}</td>\r\n                          <td>{user.employeeid}</td>\r\n                          <td>{user.employeelevel}</td>\r\n                          <td>{user.phonenumber}</td>\r\n                          <td>{user.bankaccountnumber}</td>\r\n                          <td>{user.gender}</td>\r\n                          <td>{user.hiredate}</td>\r\n                          <td>\r\n                             <Checkbox onChange={() => onCheckboxSelected(user.id)} type=\"checkbox\" checked={checboxsetState}/>\r\n                          </td>\r\n                          <td>\r\n                              <Button onClick={() => onDisableUsers(user.id)} type=\"button\" className=\"btn btn-success\">\r\n                                Disable\r\n                              </Button>\r\n                          </td>\r\n                          <td>\r\n                              <Button onClick={() => onDisableUsers(user.id)} type=\"button\" className=\"btn btn-success\">\r\n                                Enable\r\n                              </Button>\r\n                          </td>\r\n                          <td>\r\n                              <Button onClick={() => onDisableUsers(user.id)} type=\"button\" className=\"btn btn-success\">\r\n                                Edit\r\n                              </Button>\r\n                          </td>\r\n                          <td>\r\n                              <Button onClick={() => onDelete(user.id)} type=\"button\" className=\"btn btn-danger\">\r\n                                Delete\r\n                              </Button>\r\n                          </td>\r\n                          <td>\r\n                              <Button onClick={() => onSendLoginDetails(user.email,user.password)} type=\"button\" className=\"btn btn-danger\">\r\n                                SendLoginDetails\r\n                              </Button>\r\n                          </td>\r\n                        </tr>\r\n                        )}\r\n                    </tbody>\r\n                </table>\r\n            </div> \r\n        );\r\n    }\r\n}\r\n\r\nclass Checkbox extends Component{\r\n    render(){\r\n        const{onChange,type,checked} = this.props;\r\n        return(\r\n            <input type={type} onChange={onChange} checked={checked}/>\r\n        );\r\n    }\r\n}\r\n\r\nclass Button extends Component{\r\n    render(){\r\n        const{onClick,className,type,children} = this.props;\r\n        return(\r\n            <button onClick={onClick} type={type} className={className}>\r\n                {children}\r\n            </button>\r\n        );\r\n    }\r\n}\r\n\r\nexport default viewusers;\r\n\r\n","C:\\Users\\STHIGWGFWB\\Desktop\\Payroll UI\\payrollui\\src\\users\\Usermain.js",[],"C:\\Users\\STHIGWGFWB\\Desktop\\Payroll UI\\payrollui\\src\\API_URLS\\index.js",[],"C:\\Users\\STHIGWGFWB\\Desktop\\Payroll UI\\payrollui\\src\\login\\loginform.js",["122","123","124","125","126","127","128","129","130","131","132","133","134","135","136","137","138","139","140","141","142","143","144","145"],"import React, { Component } from 'react'\r\nimport GoogleLogin from 'react-goo